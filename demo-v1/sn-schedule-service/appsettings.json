{
  "AllowedHosts": "*",
  "spring": {
    "application": {
      "name": "schedule-service"
    },
    "cloud": {
      "config": {
        "uri": "http://localhost:8091",
        "validateCertificates": false
      }
    }
  },
  "eureka" : {
    "client" : {
      "shouldRegisterWithEureka" : true,
      "serviceUrl" : "http://localhost:8761/eureka",
      "ValidateCertificates":  false
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  }
}